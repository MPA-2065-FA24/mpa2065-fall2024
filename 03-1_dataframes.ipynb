   "source": [
    "vio_tbl <- arrests_df %>%\n",
    "    filter(\n",
    "        statute_desc != \"\",\n",
    "        statute_desc != \"NULL\", \n",
    "        year == 2021\n",
    "    ) %>%\n",
    "    group_by(statute_desc) %>%\n",
    "    summarise(\n",
    "        n_vios = n(),\n",
    "        prop_male = mean(gender == \"Male\"),\n",
    "        mean_age = mean(age),\n",
    "        prop_summer = mean(is_summer(month))\n",
    "    ) %>%\n",
    "    arrange(desc(n_vios))\n",
    "\n",
    "head(vio_tbl)"
   ]
